{
  "manifest_version": 3,
  "name": "Junk Filter",
  "version": "1.0",
  "description": "Removes junk content from web pages.",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://www.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
